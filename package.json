{
  "name": "node-red-contrib-graylog",
  "version": "1.2.0",
  "description": "NodeRED Graylog",
  "author": "Franciszek Madej <franciszek.madej@kolomololabs.com>",
  "license": "GPL-3.0",
  "private": false,
  "scripts": {
    "release": "standard-version",
    "test": "nyc --reporter=lcov mocha --recursive ./graylog*/*test.js",
